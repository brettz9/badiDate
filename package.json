{
  "name": "badidate",
  "version": "2.1.2",
  "description": "A library for dates in the Badí' (or Bahá'í) calendar.",
  "main": "dist/localBadiDate-mss-locales.js",
  "module": "src/localBadiDate.js",
  "bin": {
    "badi-date": "./bin/badi-date.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cli": "./bin/badi-date.js",
    "clp": "command-line-publish -c bin/optionDefinitions.js cli.svg",
    "eslint": "eslint .",
    "rollup": "rollup -c",
    "start": "static -p 8048",
    "coverage": "open-cli http://localhost:8048/coverage/ && npm start",
    "mocha": "_mocha --timeout 10000 test/tests.js --delay --require esm --require test/node-env.js",
    "browser-test": "open-cli http://localhost:8048/test/ && npm start",
    "node-test": "npm run eslint && npm run rollup && npm run mocha",
    "test-cov": "npm run eslint && npm run rollup && rm -Rf node_modules/.cache && nyc npm run mocha",
    "test": "npm run node-test"
  },
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "command-line-basics": "^0.6.2",
    "meeussunmoon": "^2.1.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "command-line-publish": "^0.6.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "esm": "^3.2.25",
    "mocha": "^7.0.1",
    "node-static": "0.7.11",
    "nyc": "^15.0.0",
    "open-cli": "^5.0.0",
    "rollup": "1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^5.2.0"
  },
  "engines": {
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janrg/badiDate.git"
  },
  "keywords": [
    "Badí'",
    "calendar",
    "Bahá'í",
    "calendar"
  ],
  "author": "Jan Greis",
  "contributors": [
    "Brett Zamir"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janrg/badiDate/issues"
  },
  "homepage": "https://github.com/janrg/badiDate#readme"
}
